{
  "manifest_version": 2,

  "name": "Example Chrome Extension",
  "description": "Example chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "history"
  ],

  "background": {
        "scripts": ["jquery.js", "background.js"]
  }
}
